{ 
	"include": [
		"./*",
	],
	"compilerOptions":{ 
		"target": "ES2017",
		"module": "ES2022",
		"moduleResolution": "bundler",
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"noImplicitReturns": true,
		"preserveSymlinks": true,
		"jsx": "react-jsx",
		"jsxImportSource": "stagnate"
	}
}
