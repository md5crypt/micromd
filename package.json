{
	"name": "micromd",
	"version": "1.0.1",
	"description": "a 3kb fully linear markdown parser",
	"main": "./cjs/index.js",
	"module": "./mjs/index.js",
	"type": "module",
	"exports": {
		".": {
			"require": "./cjs/index.js",
			"import": "./mjs/index.js"
		},
		"./*": {
			"require": "./cjs/*",
			"import": "./mjs/*"
		}
	},
	"scripts": {
		"build": "tsc",
		"start": "tsc --watch",
		"test": "node ./test/index.mjs",
		"prepublishOnly": "node publish.mjs verify && tsc --outDir mjs && tsc --outDir cjs --module commonjs && node ../test/index.mjs ./mjs/htmlRenderer.js",
		"pub": "node publish.mjs pre-publish && cd dist-package && npm publish && cd .. && node publish.mjs post-publish"
	},
	"author": "Marek Korzeniowski",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/md5crypt/micromd/issues"
	},
	"homepage": "https://github.com/md5crypt/micromd#readme",
	"keywords": [
		"markdown",
		"AST",
		"parser"
	],
	"sideEffects": false,
	"devDependencies": {
		"typescript": "^5.9.2"
	}
}
